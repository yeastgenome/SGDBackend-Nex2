  "scripts": {
    "start": "webpack-dev-server --history-api-fallback --port 2992 --hot --inline",
    "prebuild": "npm run clean && npm run copy",
    "build:dev": "cross-env DEMO_ENV=development NODE_ENV=production PREVIEW_URL=https://preview.qa.yeastgenome.org  webpack -p --optimize-minimize",
